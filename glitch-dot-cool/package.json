    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "cssjson": "^2.1.3",
    "gatsby": "^2.7.5",
    "gatsby-background-image": "^0.8.3",
    "gatsby-cli": "^2.6.7",
    "gatsby-image": "^2.2.4",
